{
  "manifest_version": 3,
  "name": "多搜索引擎",
  "version": "1.0",
  "description": "同时使用多个搜索引擎进行搜索",
  "default_locale": "zh_CN",
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "omnibox": {
    "keyword": "ms"
  },
  "background": {
    "service_worker": "background.js"
  }
} 